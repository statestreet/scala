create lift project:


mvn archetype:generate -DarchetypeGroupId=net.liftweb -DarchetypeArtifactId=lift-archetype-basic_2.8.1     -DarchetypeVersion=2.2 -DarchetypeRepository=http://scala-tools.org/repo-releases     -DremoteRepositories=http://scala-tools.org/repo-releases -DgroupId=your.group.id -DartifactId=lift_demo -Dversion=1.0

